{
    "name": "Pietro extension",
    "version": "1.0",
    "key": "",
    "description": "test",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": ["declarativeContent","storage","tabs","https://*.force.com/*", "https://*.salesforce.com/*"],
    "optional_permissions": ["contentSettings"],
    "content_scripts" : [
      {
      "matches" : ["https://*.salesforce.com/*", "https://*.force.com/*"],
      "js" : ["lib/jquery-3.4.1.min.js","script.js"]
      }
    ],
    "background": {
        "scripts": ["lib/jquery-3.4.1.min.js","background.js"],
        "persistent": false
      },
    "browser_action": {

        "default_icon": {
            "16": "images/mail_icon.png",
            "32": "images/mail_icon.png",
            "48": "images/mail_icon.png",
            "128": "images/mail_icon.png"
          }
    },
    "icons": {
        "16": "images/mail_icon.png",
        "32": "images/mail_icon.png",
        "48": "images/mail_icon.png",
        "128": "images/mail_icon.png"
      },
    "manifest_version": 2,
    "author": "Pietro Santoro"
  }