{
    "name": "Mailinda",
    "version": "1.1.1",
    "key": "",
    "description": "",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
      "notifications",
      "background",
      "declarativeContent",
      "storage",
      "tabs",
      "unlimited_storage",
      "management",
      "webNavigation",
      "webRequest", "webRequestBlocking",
      "clipboardRead",
      "<all_urls>"
    ],
    "optional_permissions": ["contentSettings"],
    "content_scripts" : [
      {
      "matches" : [
        "https://smbsalesimplementation.my.salesforce.com/*"
      ],
      "all_frames": true,
      "js" : ["lib/jquery-3.4.1.min.js","script.js"]
      }
    ],
    "background": {
        "scripts": ["lib/jquery-3.4.1.min.js","lib/jquery.tabletojson.js","background.js"],
        "persistent": true
      },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/mail_icon.png",
            "32": "images/mail_icon.png",
            "48": "images/mail_icon.png",
            "128": "images/mail_icon.png"
          }
    },
    "icons": {
        "16": "images/mail_icon.png",
        "32": "images/mail_icon.png",
        "48": "images/mail_icon.png",
        "128": "images/mail_icon.png"
      },
    "manifest_version": 2,
    "author": "Tag Team"
  }